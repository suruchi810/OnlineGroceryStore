{
  "builds": [
    {
      "src": "grocery/wsgi.py",
      "use": "@vercel/python",
      "config": {"maxLambdaSize": "15mb", "runtime": "python3.13.5"}
    }, 
    {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
            "disDir": "staticfiles_build"
        }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "grocery/wsgi.py"
    }
  ]
}

